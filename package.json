    "debug": "~4.1.1",
    "express": "~4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.10",
    "morgan": "~1.10.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
